<div class="reg-forms">
  <form id="login_user_form" action="/sessions/login" method="POST">
    <input type="text" name="user[email]" placeholder="EMAIL" autofocus/>
    <input type="password" name="user[password]" placeholder="PASSWORD"/>
    <input type="submit" id="submit-btn" />
  </form>
</div>

<%if @errors%>
   <span><%=@errors%>, Please Try Again (or Register)</span>
<%end%>
