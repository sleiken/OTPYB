<% if logged_in? %>
   <span><%= current_user.username %></span>
   <form id="logout_user_form" action="/sessions/logout" method="POST">
      <input type="hidden" name="_method" value="DELETE">
      <input type="submit" value="Logout">
   </form>
<% else %>
  <div id="registration-login-links">
    <div class="hidden" id="append-div"></div>
    <a class="btn" id="search-btn" href="/users/new">SEARCH</a>
    <div class="headerDivider"></div>
    <a class="btn" href="/places/new"><img id="header-logo" src="/seuss.jpg" /></a>
    <div class="headerDivider"></div>
    <a class="btn" id="signup-btn" href="/sessions/login">LOGIN</a>
  </div>
<% end %>
